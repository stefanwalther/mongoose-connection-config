{
  "name": "mongoose-connection-config",
  "version": "0.1.0",
  "description": "Helper library to set mongoose connection configuration.",
  "keywords": [
    "config",
    "mongoose"
  ],
  "homepage": "https://github.com/stefanwalther/mongoose-config#readme",
  "bugs": {
    "url": "https://github.com/stefanwalther/mongoose-config/issues"
  },
  "license": "MIT",
  "author": "Stefan Walther <swr.nixda@gmail.com> (http://qliksite.io)",
  "main": "./src/index.js",
  "directories": {
    "doc": "docs",
    "test": "test",
    "src": "src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefanwalther/mongoose-config.git"
  },
  "scripts": {
    "lint": "npm run lint:src && yarn run lint:test",
    "lint:fix": "npm run lint:src:fix && npm run lint:test:fix",
    "lint:src": "eslint src",
    "lint:src:fix": "eslint src --fix",
    "lint:test": "eslint test",
    "lint:test:fix": "eslint test --fix",
    "test": "npm run test:unit ",
    "test:unit": "nyc --reporter=lcov mocha './test/unit/**/*.spec.js' --require './test/mocha.conf.js' && npm run coverage",
    "coverage": "nyc report"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "3.0.0",
    "eslint": "4.18.1",
    "eslint-config-space-single": "0.3.1",
    "mocha": "5.1.1",
    "nyc": "11.4.1"
  },
  "renovate": {
    "extends": [
      "config:base"
    ]
  }
}
